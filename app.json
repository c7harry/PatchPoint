{
  "expo": {
    "name": "PatchPoint",
    "slug": "patchpoint",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/Favicon.png",
    "scheme": "patchpoint",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/Favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f172a"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Favicon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/Favicon.png"
    }
  }
}
